package exam_templates;

public class digitos {

	public static void main(String[] args) {
		int num_copia=0,num=0,digito=0;
		String cad="",cad_dig="";
		num_copia=num;
		while(num!=0) {
			digito = num%10;
			num=num/10;
			cad_dig=cad_dig+digito;
		}//end while
		
		cad="Los d√≠gitos de "+num_copia+"son: "+digito+". \n";
	
	}

}
