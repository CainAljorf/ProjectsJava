package exam_templates;

import javax.swing.JOptionPane;

public class divisores {

	public static void main(String[] args) {
		int num=0;
		String num_input;
		String cad,cad_div= "";
		num=49;
		for(int i=1;i<=num;i++) {
			if((num%i)==0) {
				cad_div=cad_div+"\n"+i;
			}
		}
		cad= "Los divisores de "+num+" son: ";
		JOptionPane.showMessageDialog(null, cad+cad_div);
	}

}
